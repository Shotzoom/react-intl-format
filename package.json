{
  "name": "react-intl-format",
  "version": "0.1.3",
  "description": "A react-intl component that provides a alternate way to localize your components.",
  "keywords": [
    "intl",
    "i18n",
    "internationalization",
    "locale",
    "localization",
    "globalization",
    "react",
    "reactjs",
    "format",
    "formatting",
    "translate",
    "translation"
  ],
  "main": "./lib/react-intl-format.js",
  "module": "./lib/react-intl-format.es.js",
  "jsnext:main": "./lib/react-intl-format.es.js",
  "author": "Brenden Snyder <brendens@shotzoom.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/edufii/react-intl-format.git"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rimraf lib bin",
    "build": "npm run clean && npm run build:lib && npm run build:umd && npm run build:umd:min",
    "build:lib": "rollup --config rollup.config.js",
    "build:umd": "cross-env NODE_ENV=development rollup --config rollup.config.umd.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup --config rollup.config.umd.js",
    "lint": "eslint --ext .jsx --ext .js .",
    "lint:fix": "eslint --ext .jsx --ext .js --fix .",
    "test": "cross-env BABEL_ENV=test jest",
    "test:start": "cross-env BABEL_ENV=test jest --watch",
    "prepublish": "npm run test && npm run lint && npm run build"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-es2015-block-scoped-functions": "^6.22.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.26.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-function-name": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "cross-env": "^5.0.5",
    "enzyme": "^2.9.1",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.3.0",
    "jest": "^21.1.0",
    "prettier": "^1.7.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-intl": "^2.4.0",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1"
  },
  "peerDependencies": {
    "react-intl": "^2.4.0",
    "prop-types": "^15.5.10"
  },
  "dependencies": {
    "invariant": "^2.2.2"
  }
}
